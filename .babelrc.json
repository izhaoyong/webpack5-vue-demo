{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browser": [
                        "> 1%",
                        "last 2 versions"
                    ]
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": 3
            }
        ]
    ]
}
